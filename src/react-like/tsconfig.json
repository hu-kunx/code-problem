{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "lib": [
      "DOM",
      "ES7",
      "ES2019.Object",
      "ES2017.Object"
    ],
    "jsx": "react",
    "jsxFactory": "h",
    "strict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "declarationDir": "./types",
    "declaration": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["index.tsx"],
  "exclude": ["src"]
}
